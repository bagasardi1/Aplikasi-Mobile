  "item": [
    {
      "name": "üîê USER AUTHENTICATION",
      "item": [
        {
          "name": "1. Register User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email_user\": \"user@example.com\",\n  \"nama_user\": \"John Doe\",\n  \"no_telp\": \"081234567890\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/register/user",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "register", "user"]
            },
            "description": "Register user baru dengan email, nama, nomor telepon, dan password"
          },
          "response": []
        },
        {
          "name": "2. Login User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email_user\": \"user@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/login/user",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "login", "user"]
            },
            "description": "Login sebagai user dan dapatkan token Sanctum"
          },
          "response": []
        },
        {
          "name": "3. Get User Dashboard",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{USER_TOKEN}}"
              },
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{BASE_URL}}/api/dashboard/user",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "dashboard", "user"]
            },
            "description": "Akses dashboard user (memerlukan valid token)"
          },
          "response": []
        },
        {
          "name": "4. Logout User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{USER_TOKEN}}"
              },
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{BASE_URL}}/api/logout/user",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "logout", "user"]
            },
            "description": "Logout user dan hapus semua tokens"
          },
          "response": []
        }
      ]
    },
